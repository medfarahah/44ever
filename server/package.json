{
  "name": "44ever-backend",
  "version": "1.0.0",
  "description": "Backend API for 44ever e-commerce platform",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "db:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "db:generate": "prisma generate --schema=prisma/schema.prisma",
    "db:push": "prisma db push --schema=prisma/schema.prisma",
    "db:studio": "prisma studio --schema=prisma/schema.prisma",
    "db:seed": "node database/init.js",
    "create-admin": "node scripts/create-admin.js",
    "make-admin": "node scripts/make-admin.js"
  },
  "keywords": [
    "express",
    "api",
    "ecommerce"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/neon-js": "^0.1.0-beta.21",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "react-router-dom": "^7.12.0",
    "uuid": "^9.0.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1"
  }
}
